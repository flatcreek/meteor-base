{
  "name": "meteor-base",
  "version": "5.0.0",
  "description": "A foundation for Meteor React projects.",
  "private": true,
  "repository": {
    "url": "git@github.com:allenfuller/meteor-base.git",
    "type": "git"
  },
  "author": "Allen Fuller <meteor@allenfuller.com>",
  "license": "MIT",
  "meteor": {
    "mainModule": {
      "client": "imports/startup/client/index.js",
      "server": "imports/startup/server/index.js"
    }
  },
  "scripts": {
    "start": "meteor --settings settings-dev.json",
    "start-all": "redis-server /usr/local/etc/redis.conf & meteor --settings settings-dev_worker.json",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "backup": "chmod +x .mongodb-backups/dump && ./.mongodb-backups/dump",
    "restore": "chmod +x .mongodb-backups/restore && ./.mongodb-backups/restore"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@faker-js/faker": "^8.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "b64-to-blob": "^1.2.19",
    "babel-plugin-lodash": "^3.3.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.1",
    "bootstrap": "^5.3.2",
    "commonmark": "^0.29.0",
    "core-js": "^3.1.4",
    "file-saver": "^2.0.2",
    "handlebars": "^4.7.7",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.5.1",
    "jquery-validation": "^1.19.5",
    "jszip": "^3.7.0",
    "juice": "^8.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "meteor-node-stubs": "^0.4.1",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-autobind": "^1.0.6",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-ga": "^2.6.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.48.2",
    "react-markdown": "^8.0.5",
    "react-router": "^5.2.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.20.0",
    "sanitize-html": "^2.3.2",
    "simpl-schema": "^3.4.1",
    "styled-components": "^4.3.2",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "@types/react": "^18.0.35",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "meteor-jest-stubs": "^2.2.0",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "rangeStart": 0,
    "parser": "babel",
    "proseWrap": "preserve"
  }
}
